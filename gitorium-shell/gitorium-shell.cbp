<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gitorium-shell" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/gitorium-shell" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../gitorium/bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/gitorium-shell" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../gitorium/bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wall" />
			<Add option="-std=c99" />
			<Add directory="../gitorium" />
		</Compiler>
		<Linker>
			<Add library="../gitorium/bin/Debug/libgitorium.so" />
		</Linker>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../version.h" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
